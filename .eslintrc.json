{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-underscore-dangle": 0,
      "space-before-function-paren": 0,
      "func-names": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
}
